{
  "buildCommand": "pnpm build",
  "devCommand": "pnpm dev",
  "installCommand": "pnpm install",
  "framework": "other",
  "outputDirectory": "dist",
  "public": true,
  "env": {
    "DATABASE_URL": {
      "description": "Connection string do banco de dados MySQL/TiDB"
    },
    "JWT_SECRET": {
      "description": "Secret para autenticação JWT"
    },
    "STRIPE_SECRET_KEY": {
      "description": "Chave secreta do Stripe"
    },
    "STRIPE_WEBHOOK_SECRET": {
      "description": "Secret do webhook do Stripe"
    },
    "ENCRYPTION_KEY": {
      "description": "Chave para criptografia AES-256"
    },
    "VITE_APP_TITLE": {
      "description": "Título da aplicação"
    },
    "VITE_APP_LOGO": {
      "description": "Logo da aplicação"
    },
    "VITE_STRIPE_PUBLISHABLE_KEY": {
      "description": "Chave pública do Stripe"
    },
    "VITE_FRONTEND_FORGE_API_KEY": {
      "description": "Chave API frontend do Manus"
    },
    "VITE_FRONTEND_FORGE_API_URL": {
      "description": "URL da API frontend do Manus"
    },
    "VITE_OAUTH_PORTAL_URL": {
      "description": "URL do portal OAuth do Manus"
    },
    "VITE_APP_ID": {
      "description": "ID da aplicação no Manus"
    },
    "OAUTH_SERVER_URL": {
      "description": "URL do servidor OAuth do Manus"
    },
    "OWNER_NAME": {
      "description": "Nome do casal"
    },
    "OWNER_OPEN_ID": {
      "description": "Open ID do proprietário no Manus"
    },
    "BUILT_IN_FORGE_API_KEY": {
      "description": "Chave API do Manus"
    },
    "BUILT_IN_FORGE_API_URL": {
      "description": "URL da API do Manus"
    }
  },
  "rewrites": [
    {
      "source": "/api/trpc/:path*",
      "destination": "/api/trpc/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/api/trpc/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}
